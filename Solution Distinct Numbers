#include<bits/stdc++.h> 
using namespace std; 

int main(){
	 ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  
	int n;
	cin>>n;
long long 	int a[n];
long long count=1;

	for(int i=0;i<n;i++){
		cin>>a[i];
		
		
	}
	
	sort(a,a+n);
	long long int x=a[0];
	for(int i=1;i<n;i++){
if(x!=a[i]){
	x=a[i];
	count++;
}		
		
	}
	cout<<count;
	
    
}
